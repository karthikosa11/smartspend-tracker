{
  "name": "smartspend-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd frontend && vite",
    "dev:server": "tsx watch backend/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd frontend && vite build",
    "build:server": "tsc --project backend/tsconfig.json",
    "preview": "cd frontend && vite preview",
    "start": "cross-env NODE_ENV=production node backend/dist/index.js",
    "start:prod": "cross-env NODE_ENV=production tsx backend/index.ts",
    "server": "tsx backend/index.ts",
    "db:init": "tsx backend/db/init-standalone.ts"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@google/genai": "^1.30.0",
    "recharts": "^3.5.1",
    "lucide-react": "^0.555.0",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "tsx": "^4.7.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3"
  }
}
